{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "budget-tool": {
      "projectType": "application",
      "prefix": "budget",
      "root": "",
      "architect": {
        "build": {
          "builder": "",
          "options": {
            "assets": [
              "apps/budget-tool/src/favicon.ico",
              "apps/budget-tool/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared-assets",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/budget-tool/src/styles.scss",
              "libs/scss/_index.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "staging": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/environment.staging.ts"
                },
                {
                  "replace": "libs/environments/firebase/config.ts",
                  "with": "libs/environments/firebase/config.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        }
      }
    },
    "dashboard": {
      "projectType": "application",
      "root": "apps/dashboard",
      "sourceRoot": "apps/dashboard/src",
      "prefix": "dashboard",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/dashboard",
            "index": "apps/dashboard/src/index.html",
            "main": "apps/dashboard/src/main.ts",
            "polyfills": "apps/dashboard/src/polyfills.ts",
            "tsConfig": "apps/dashboard/tsconfig.app.json",
            "assets": [
              "apps/dashboard/src/favicon.ico",
              "apps/dashboard/src/assets"
            ],
            "styles": [
              "apps/dashboard/src/styles.scss",
              "libs/scss/_index.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/scss/"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/builds/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        }
      }
    },
    "extension-toolkit": {
      "projectType": "application",
      "root": "apps/extension-toolkit",
      "sourceRoot": "apps/extension-toolkit/src",
      "prefix": "picsa",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "assets": [
              "apps/extension-toolkit/src/favicon.ico",
              "apps/extension-toolkit/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared-assets/",
                "output": "./assets"
              },
              {
                "glob": "**/*",
                "input": "./apps/budget-tool/src/assets",
                "output": "./assets"
              },
              {
                "glob": "**/*",
                "input": "./apps/climate-tool/src/assets",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/extension-toolkit/src/styles.scss",
              "apps/budget-tool/src/styles.scss",
              "apps/climate-tool/src/styles.scss",
              "libs/scss/_index.scss",
              "./node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/builds/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "mw": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/environment.mw.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "dev": {
              "sourceMap": true
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "extension-toolkit:build:dev"
          },
          "configurations": {
            "mw": {
              "browserTarget": "extension-toolkit:build:mw"
            }
          }
        }
      }
    },
    "climate-tool": {
      "projectType": "application",
      "root": "apps/climate-tool",
      "sourceRoot": "apps/climate-tool/src",
      "prefix": "climate",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/climate-tool",
            "index": "apps/climate-tool/src/index.html",
            "main": "apps/climate-tool/src/main.ts",
            "polyfills": "apps/climate-tool/src/polyfills.ts",
            "tsConfig": "apps/climate-tool/tsconfig.app.json",
            "assets": [
              "apps/climate-tool/src/favicon.ico",
              "apps/climate-tool/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared-assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/climate-tool/src/styles.scss",
              "libs/scss/_index.scss",
              "./node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": []
          },
          "configurations": {
            "staging": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "mw": {
              "fileReplacements": [
                {
                  "replace": "libs/environments/environment.ts",
                  "with": "libs/environments/environment.mw.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        }
      }
    }
  }
}
