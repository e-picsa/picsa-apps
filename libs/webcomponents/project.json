{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/webcomponents/src",
  "projectType": "library",
  "generators": {
    "@nxext/stencil:component": {
      "style": "css"
    }
  },
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nxext/stencil:test",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/webcomponents/tsconfig.lib.json",
        "configPath": "libs/webcomponents/stencil.config.ts",
        "outputPath": "dist/libs/webcomponents"
      }
    },
    "build": {
      "executor": "@nxext/stencil:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/webcomponents/tsconfig.lib.json",
        "configPath": "libs/webcomponents/stencil.config.ts",
        "outputPath": "dist/libs/webcomponents"
      },
      "configurations": {
        "production": {
          "dev": false,
          "prod": true
        }
      }
    },
    "serve": {
      "executor": "@nxext/stencil:serve",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/webcomponents/tsconfig.lib.json",
        "configPath": "libs/webcomponents/stencil.config.ts",
        "outputPath": "dist/libs/webcomponents"
      }
    },
    "e2e": {
      "executor": "@nxext/stencil:e2e",
      "outputs": ["{options.outputPath}"],
      "options": {
        "projectType": "library",
        "tsConfig": "libs/webcomponents/tsconfig.lib.json",
        "configPath": "libs/webcomponents/stencil.config.ts",
        "outputPath": "dist/libs/webcomponents",
        "watch": "true"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": "libs/webcomponents/**/*.{ts,tsx}"
      }
    }
  }
}
