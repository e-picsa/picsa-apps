{
  "name": "picsa-apps-app-native-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/picsa-apps/app-native-e2e/src",
  "tags": [],
  "targets": {
    // Build web app, copy mock fixtures and compile apk
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run picsa-apps-app:build:e2e",
          "node -e \"require('fs').cpSync('apps/picsa-apps/app-native-e2e/src/fixtures/assets', 'dist/apps/picsa-apps/app/assets', {recursive: true})\"",
          "nx run picsa-apps-app-native:build-debug-apk"
        ],
        "parallel": false
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["{projectRoot}/**/*.ts"],
      "options": {
        "command": "wdio run wdio.conf.ts",
        "cwd": "apps/picsa-apps/app-native-e2e"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/picsa-apps/app-native-e2e/**/*.ts"]
      }
    }
  }
}
