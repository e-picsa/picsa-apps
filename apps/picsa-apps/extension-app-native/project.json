      "outputs": [],
      "options": {
        "command": "yarn jetify"
      }
    },
    "open": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx cap open android",
        "cwd": "apps/picsa-apps/extension-app-native"
      }
    },
    "cap": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx cap",
        "cwd": "apps/picsa-apps/extension-app-native"
      }
    }
  },
  "tags": []
