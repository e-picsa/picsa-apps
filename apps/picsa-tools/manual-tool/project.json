{
  "name": "picsa-tools-manual-tool",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/picsa-tools/manual-tool/src",
  "prefix": "picsa",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/picsa-tools/manual-tool",
        "index": "apps/picsa-tools/manual-tool/src/index.html",
        "main": "apps/picsa-tools/manual-tool/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/picsa-tools/manual-tool/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/picsa-tools/manual-tool/src/favicon.ico",
          "apps/picsa-tools/manual-tool/src/assets",
          {
            "glob": "picsa-field-manual-en.pdf",
            "input": "apps/picsa-tools/resources-tool/src/assets/resources",
            "output": "/assets/resources"
          },
          {
            "glob": "picsa-manual-farmer.pdf",
            "input": "apps/picsa-tools/resources-tool/src/assets/resources",
            "output": "/assets/resources"
          },
          {
            "glob": "{*.min.js,op-chaining-support.js,cmaps/*,images/*,locale/en-GB/*,locale/locale.properties,standard_fonts/**}",
            "input": "node_modules/ngx-extended-pdf-viewer/assets/",
            "ignore": ["**/*-es5.min.js"],
            "output": "/assets/pdf-viewer"
          }
        ],
        "styles": ["apps/picsa-tools/manual-tool/src/styles.scss", "libs/theme/src/_index.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "picsa-tools-manual-tool:build:production"
        },
        "development": {
          "browserTarget": "picsa-tools-manual-tool:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "picsa-tools-manual-tool:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/picsa-tools/manual-tool/**/*.ts", "apps/picsa-tools/manual-tool/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/picsa-tools/manual-tool/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
