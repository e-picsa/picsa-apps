{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/picsa-tools/budget-tool/src",
  "prefix": "budget",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/picsa-tools/budget-tool",
        "index": "apps/picsa-tools/budget-tool/src/index.html",
        "main": "apps/picsa-tools/budget-tool/src/main.ts",
        "polyfills": "apps/picsa-tools/budget-tool/src/polyfills.ts",
        "tsConfig": "apps/picsa-tools/budget-tool/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/picsa-tools/budget-tool/src/favicon.ico",
          "apps/picsa-tools/budget-tool/src/assets"
        ],
        "styles": [
          "apps/picsa-tools/budget-tool/src/styles.scss",
          "libs/theme/src/_index.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "picsa-tools-budget-tool:build:production"
        },
        "development": {
          "browserTarget": "picsa-tools-budget-tool:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "picsa-tools-budget-tool:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/picsa-tools/budget-tool/**/*.ts",
          "apps/picsa-tools/budget-tool/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/picsa-tools/budget-tool"],
      "options": {
        "jestConfig": "apps/picsa-tools/budget-tool/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
