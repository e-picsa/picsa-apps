{
  "name": "picsa-server",
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/picsa-server/docker",
  "prefix": "picsa",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "dependsOn": [],
      "outputs": [],
      "options": {
        "commands": ["npx supabase start"],
        "cwd": "apps/picsa-server"
      }
    },
    "start-functions": {
      "executor": "nx:run-commands",
      "dependsOn": [],
      "outputs": [],
      "options": {
        "commands": ["npx supabase functions serve"],
        "cwd": "apps/picsa-server"
      }
    },

    "gen-types": {
      "executor": "nx:run-commands",
      "dependsOn": [],
      "outputs": [],
      "options": {
        "commands": ["npx supabase gen types typescript --local > supabase/types/index.ts"],
        "cwd": "apps/picsa-server"
      }
    },
    "stop": {
      "executor": "nx:run-commands",
      "dependsOn": [],
      "outputs": [],
      "options": {
        "commands": ["npx supabase stop"],
        "cwd": "apps/picsa-server"
      }
    },
    "supabase": {
      "executor": "nx:run-commands",
      "dependsOn": [],
      "outputs": [],
      "options": {
        "commands": ["npx supabase"],
        "cwd": "apps/picsa-server"
      }
    }
  },
  "tags": []
}
