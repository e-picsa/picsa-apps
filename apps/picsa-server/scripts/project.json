{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/picsa-server/scripts",
  "prefix": "picsa",
  "implicitDependencies": ["picsa-server-cloud-functions"],
  "targets": {
    "docker-prepare": {
      "executor": "./tools/executors/execa:commandSync",
      "dependsOn": [{ "projects": "dependencies", "target": "build" }],
      "outputs": ["apps/picsa-server/docker/cloud-functions"],
      "options": {
        "commands": ["ts-node apps/picsa-server/scripts/docker-prepare.ts"]
      }
    },
    "db-migrate": {
      "executor": "./tools/executors/execa:commandSync",
      "outputs": [],
      "options": {
        "commands": ["ts-node apps/picsa-server/scripts/db-migrate.ts"]
      }
    },
    "type-definitions-generate": {
      "executor": "./tools/executors/execa:commandSync",
      "outputs": [],
      "options": {
        "commands": [
          "ts-node apps/picsa-server/scripts/type-definitions-generate.ts"
        ]
      }
    }
  },
  "tags": []
}
